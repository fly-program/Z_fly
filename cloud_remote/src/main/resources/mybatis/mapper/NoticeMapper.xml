<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.dao.NoticeDao">
    <select id="findByNoticeName" parameterType="String" resultType="Notice">
        select * from notice where notice_name = #{notice_name}
    </select>
</mapper>